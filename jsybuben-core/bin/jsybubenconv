#!/usr/bin/env node

"use strict";

var jsybuben = require("../lib/api");

var readline = require("readline");
var rl = readline.createInterface({
    input: process.stdin,
    output: process.stdout,
    terminal: false
});

rl.on("line", function(line) {
    var converted = jsybuben.toAybuben(line);
    console.log(converted);
});
